{
    "name": "alanbem/josser",
    "description": "JSON-RPC client for PHP",
    "keywords": ["json-rpc", "jsonrpc", "rpc", "json", "client"],
    "homepage": "http://github.com/alanbem/josser",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alan Gabriel Bem",
            "email": "alan.bem@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "guzzlehttp/guzzle": "^5.0|^6.0",
        "symfony/serializer": "^2.0|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0"
    },
    "autoload": {
        "psr-0": {
            "Josser": ["src/", "tests/"]
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
